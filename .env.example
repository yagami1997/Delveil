# Delveil Environment Variables
# Make a copy of this file named .env and fill in your values

# Admin password for La Jolla control panel
ADMIN_PASSWORD=your_secure_password_here

# Secret key for token generation and encryption
SECRET_KEY=your_random_secret_key_here

# Playwright browser path (optional)
# PLAYWRIGHT_BROWSERS_PATH=/path/to/playwright/browsers

# Server configuration
PORT=8000
HOST=127.0.0.1

# Debug mode (set to false in production)
DEBUG=false
